<div class="container blogpage">
  <h1>Login</h1>


  <%= form_for(:session, url: login_path, method: 'POST') do |f| %>
    <div class="field"><%= f.text_field :username, placeholder: 'Email' %><br></div>
    <div class="field"><%= f.password_field :password, placeholder: 'Password' %><br></div>
    <div class="submit"><%= f.submit 'Submit'%></div>
  <% end %>
</div>
